            <h3 id="dynamic1" style="color:blue">

              </h3>
            <h5 style="color:blue">
                sentiment: <%= recommend.sentiment %>
                score: <%= recommend.score %>
              </h5>
            <span class="stars"><%= recommend.score*5 %></span>
            <br>
            <img alt="" id="my_image1" style="width:200;height:200" />
            <br>
            <br> Ingredient:
            <p id="ingredient1">

            </p>
