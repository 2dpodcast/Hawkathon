  <!-- end container -->
  <div class="container" id="recommend">
    <div class="mdl-card mdl-cell mdl-cell--12-col">
      <div class="mdl-card__supporting-text mdl-grid mdl-grid--no-spacing">
        <h4 class="mdl-cell mdl-cell--12-col" style="text-align:center;padding-top:20px">Recommendation</h4>
        <div class="section__circle-container mdl-cell mdl-cell--2-col mdl-cell--1-col-phone">
          <div class="section__circle-container__circle mdl-color--primary">
          </div>
        </div>

        <div id="tabs">
          <ul>
            <li><a href="#tabs-1">Food 1</a></li>
            <li><a href="#tabs-2">Food 2</a></li>
            <li><a href="#tabs-3">Food 3</a></li>
          </ul>
